{
    "name": "obsidian-jira-issue",
    "version": "1.18.0",
    "description": "This plugin allows you to track the progress of [Atlassian Jira](https://www.atlassian.com/software/jira) issues from your [Obsidian](https://obsidian.md/) notes.",
    "main": "src/main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json"
    },
    "keywords": [
        "obsidian",
        "Jira",
        "Atlassian"
    ],
    "author": "marc0l92",
    "license": "MIT",
    "devDependencies": {
        "@types/jsonpath": "^0.2.0",
        "@types/ms": "^0.7.31",
        "@types/node": "^16.11.36",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "builtin-modules": "^3.2.0",
        "esbuild": "0.13.12",
        "jsonpath": "^1.1.1",
        "moment": "^2.29.3",
        "ms": "^2.1.3",
        "obsidian": "latest",
        "tslib": "2.3.1",
        "typescript": "4.4.4"
    },
    "dependencies": {
        "@codemirror/language": "^0.20.2",
        "@codemirror/rangeset": "^0.19.9",
        "@codemirror/state": "^0.20.0",
        "@codemirror/stream-parser": "^0.19.9",
        "@codemirror/view": "^0.20.6",
        "@lezer/common": "^0.16.0"
    }
}
